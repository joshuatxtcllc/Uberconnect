# .env - Environment variables for Uber integration

# Server Configuration

PORT=3000
NODE_ENV=production

# Uber API Credentials

# Get these from https://developer.uber.com/

UBER_CLIENT_ID=your_uber_client_id_here
UBER_CLIENT_SECRET=your_uber_client_secret_here
UBER_CUSTOMER_ID=your_uber_customer_id_here

# Shop Information

SHOP_ADDRESS=123 Main St, Houston, TX 77001, USA
SHOP_PHONE=+1234567890

# Optional: Webhook verification

UBER_WEBHOOK_SECRET=your_webhook_secret_here

# Database (if using)

DATABASE_URL=postgresql://username:password@localhost:5432/jaysframes

# Notification Services (optional)

TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=+1234567890

SENDGRID_API_KEY=your_sendgrid_key
FROM_EMAIL=orders@jaysframes.com
